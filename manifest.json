{
  // Required
    "manifest_version": 2,
    "name": "Geometry Dash Music",
    "version": "0.0.1",

    // Recommended
    "description": "An extension with the music from geometry dash",

    // Pick one (or none)
    "browser_action": {
        "default_icon": {                    // optional
            "32": "assets/icon.png"          // optional
        },
        "default_title": "Geo Dash Music",      // optional; shown in tooltip
        "default_popup": "popup.html"        // optional
    },
    "author": "JDB",
    "homepage_url": "https://scratch.mit.edu/projects/104551671/",
    "icons": {
        "16":"assets/icon16.png"
    },
  //  "options_ui": {
//        "chrome_style": true,
  //      "page": "options.html"
    //},
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+G"
            }
        }
    }
}